{
  "name": "slidev-theme-eloc",
  "version": "1.0.2",
  "description": "Focus on writing, present in a concise style.",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "license": "MIT",
  "repository": "https://github.com/zthxxx/slides/blob/master/packages/slidev-theme-eloc",
  "homepage": "https://eloc-slidev.vercel.app",
  "scripts": {
    "dev": "slidev example.md --open",
    "clean": "rimraf dist",
    "build:example": "npm run clean && slidev build example.md",
    "export": "slidev export screenshot.md --output export",
    "screenshot": "slidev export screenshot.md --output screenshot --format png",
    "deploy": "npm run build:example && vercel --cwd dist"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "example.md",
    "components",
    "layouts",
    "public",
    "setup",
    "styles",
    "vite.config.ts",
    "*.vue"
  ],
  "dependencies": {
    "@slidev/types": "^0.49.29",
    "@vueuse/core": "^10.2.1",
    "@vueuse/math": "^10.2.1",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.4",
    "prism-themes": "^1.9.0",
    "theme-vitesse": "^0.7.2"
  },
  "devDependencies": {
    "@slidev/cli": "0.49.29",
    "playwright-chromium": "1.35.1",
    "postcss": "8.4.25",
    "rimraf": "5.0.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.30.0 <0.50.0"
  },
  "// slidev theme meta": "https://github.com/slidevjs/slidev/blob/v0.49.29/packages/types/src/types.ts#L72",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "both",
    "// slidev config": "https://github.com/slidevjs/slidev/blob/v0.49.29/packages/types/src/config.ts#L10",
    "defaults": {
      "routerMode": "hash",
      "canvasWidth": 1580,
      "selectable": true,
      "highlighter": "prism",
      "fonts": {
        "mono": "Fira Code"
      }
    }
  }
}
